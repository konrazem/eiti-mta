{
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/logout"
  },
  "routes": [
    {
      "destination": "eiti-client",
      "source": "^/client/(.*)$",
      "authenticationType": "xsuaa"
    },
    {
      "destination": "eiti-server",
      "source": "^/server/(.*)$",
      "authenticationType": "xsuaa"
    }
  ]
}