{
  "welcomeFile": "/index.html",
  "sessionTimeout": 10,
  "routes": [
    {
      "source": "^/(.*)$",
      "localDir": "resources"
    },
    {
      "destination": "eiti-server",
      "source": {
        "path": "^/api/v1/(.*)$",
        "matchCase": false
      },
      "authenticationType": "xsuaa"
    },
    {
      "destination": "eiti-app-build",
      "source": "^/(.*)$",
      "authenticationType": "xsuaa"
    }
  ]
}